{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Candidatos</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Surnames</th>
                <td>{{ entity.surnames }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Documentnumber</th>
                <td>{{ entity.documentNumber }}</td>
            </tr>
            <tr>
                <th>Department</th>
                <td>{{ entity.department }}</td>
            </tr>
            <tr>
                <th>Votes</th>
                <td>{{ entity.votes }}</td>
            </tr>
            <tr>
                <th>Registerdate</th>
                <td>{{ entity.registerDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updatedate</th>
                <td>{{ entity.updateDate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('candidatos') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('candidatos_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
